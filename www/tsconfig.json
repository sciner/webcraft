{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": false,
        "removeComments": false,
        "sourceMap": false,
        "strict": false,
        "target": "es2022",
        "module": "es2022",
        "noUnusedLocals": true,
        "inlineSourceMap": true,
        "noUnusedParameters": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "outDir": "js"
    },
    "files": [
        "src/package.json"
    ],
    "include": [
        "src/**/*"
    ]
}